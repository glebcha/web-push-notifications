{
  "name": "web-push-notifications",
  "version": "0.0.1",
  "description": "Push notifications library",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "author": "Glebcha",
  "license": "MIT",
  "repository": "https://github.com/glebcha/web-push-notifications.git",
  "scripts": {
    "build": "rollup -c && yarn changelog",
    "build:dev": "rollup -c --watch",
    "lint": "eslint src --ext .ts",
    "preversion": "yarn lint",
    "postversion": "yarn build",
    "start": "http-server public --cors",
    "test": "jest --passWithNoTests",
    "changelog": "changelog/generate-changelog",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "docs": "typedoc --out docs --readme none --target ES6 --mode file ./src"
  },
  "dependencies": {
    "nanoid": "^3.1.31"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@firebase/app-types": "^0.5.0",
    "@firebase/messaging-types": "^0.4.0",
    "@firebase/util": "^0.2.36",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@size-limit/preset-big-lib": "^4.0.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.23",
    "@types/nanoid": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.1-alpha.18",
    "@typescript-eslint/parser": "^2.0.0",
    "@wessberg/rollup-plugin-ts": "^1.2.19",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "git-changelog": "^1.1.2",
    "http-server": "^0.12.0",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rollup": "^1.18.0",
    "rollup-plugin-size-snapshot": "^0.11.0",
    "rollup-plugin-terser": "^5.1.1",
    "simple-git": "^1.126.0",
    "size-limit": "^4.0.0",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.17.1",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.6.2"
  }
}
